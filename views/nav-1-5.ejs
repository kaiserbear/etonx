<script>
//Static Campaigns
window.dataLayer = window.dataLayer || [];
var isEventTriggered = function(name) {
return name.toLowerCase().indexOf('eventtriggered') > -1;
};
var ssSessionKey = 'SS_WATTS_SESSION';
var variationsWithNames = '';
var variationIdsOnPage = '';
var currentVisitCount = '30';
//First Visit
if (parseInt(currentVisitCount) === 1 && localStorage.getItem(ssSessionKey) === null) {
localStorage.setItem('SS_Visit_Count', 1);
}
//New Visit - clear session
else if (parseInt(currentVisitCount) !== parseInt(localStorage.getItem('SS_Visit_Count'))) {
localStorage.setItem('SS_Visit_Count', currentVisitCount);
localStorage.removeItem(ssSessionKey);
}
var variationIds = variationIdsOnPage.split('|');
var variationNames = variationsWithNames.split(':');
var ssSessionData = localStorage.getItem(ssSessionKey) || '';
var relevantVariationIdsOnPage = variationIds.filter(function(id, i) {
return parseInt(id) !== 0 && !isEventTriggered(variationNames[i]);
});
var newVariationsOnPage = relevantVariationIdsOnPage.filter(function(id, i) {
return relevantVariationIdsOnPage.indexOf(id) === i && // exclude duplicate IDs
ssSessionData.indexOf(id) === -1; // exclude already counted IDs
});
var newVariationsOnPageString = newVariationsOnPage.join('|');
var allSessionVariations = [ssSessionData, newVariationsOnPageString].filter(Boolean).join('|');
localStorage.setItem(ssSessionKey, allSessionVariations);
// Push ALL session hits
if (allSessionVariations) {
if (allSessionVariations.length < 150) {
window.dataLayer.push({
ss_variant_hit: newVariationsOnPageString,
ss_variants: allSessionVariations,
ss_user_id: 'G6646305394958401120',
});
} else {
window.dataLayer.push({
event: 'exp_event',
eventCategory: 'Sitespect',
eventAction: 'Variant limit reached',
eventLabel: allSessionVariations,
ss_variant_hit: newVariationsOnPageString,
ss_user_id: 'G6646305394958401120',
});
}
}
